{
  "name": "angular-alerts",
  "version": "0.0.1",
  "description": "A simple alert directive and service for angular.",
  "main": "index.html",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a 0.0.0.0 -p 8000",
    "pretest": "npm install",
    "test": "node node_modules/karma/bin/karma start test/karma.conf.js",
    "test-single-run": "node node_modules/karma/bin/karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentendo/angular-alerts.git"
  },
  "keywords": [
    "angular",
    "alert"
  ],
  "author": "kentendo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kentendo/angular-alerts/issues"
  },
  "homepage": "https://github.com/kentendo/angular-alerts",
  "devDependencies": {
    "grunt": "latest",
    "grunt-contrib-cssmin": "latest",
    "grunt-contrib-htmlmin": "latest",
    "grunt-contrib-jasmine": "^0.8.2",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-jscs": "^1.6.0",
    "grunt-karma": "^0.10.1",
    "jasmine-core": "^2.2.0",
    "jshint": "latest",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.8",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4"
  }
}
